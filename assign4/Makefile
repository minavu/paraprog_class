#----------------------------------------------------------------------
# For CS415P/515 MPI Lab
#----------------------------------------------------------------------

%: %.c
	gcc -DDEBUG -g -o $@ $< -lm

%-mpi: %-mpi.c
	mpicc -DDEBUG -g -o $@ $< -lm

%-mpi2: %-mpi2.c
	mpicc -DDEBUG -g -o $@ $< -lm
